a<-1:1000 
b<-atan(1/a)

x<-1:1000
x<-as.numeric(x)
y<-atan(1/x)
z<-1/tan(y)

x==z
identical(x,z)
all.equal(x,z)
all.equal(x,z,tolerance = 0)

true_and_missing<-c(TRUE,NA)
false_and_missing<-c(FALSE,NA)
any(true_and_missing,false_and_missing,mixed)
all(true_and_missing,false_and_missing,mixed)


options(digits = 1)
